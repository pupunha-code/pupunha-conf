{
  "expo": {
    "name": "Pupunha Conf",
    "slug": "pupunha-conf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "pupunhaconf",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pupunhaconf.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.pupunhaconf.app",
      "adaptiveIcon": {
        "backgroundColor": "#201C20"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
       [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 25
          }
        }
      ],
    "@logrocket/react-native",
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#201C20",
          "dark": {
            "backgroundColor": "#201C20"
          }
        }
      ],
      "expo-navigation-bar"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "root": "./src/app"
      },
      "expo-navigation-bar": {},
      "eas": {
        "projectId": "3ac67d0d-f637-498f-88a7-36738f84a715"
      }
    }
  }
}
